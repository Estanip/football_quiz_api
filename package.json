{
  "name": "footbal_quiz_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "predeploy": "yarn install && yarn run build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"src/__test__/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "postinstall": "husky",
    "db:migration:create": "cross-var ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:create ./src/database/migrations/$npm_config_name",
    "db:migration:generate": "cross-var npx ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:generate ./src/database/migrations/$npm_config_name -d src/database/data-source.ts",
    "db:migration:revert": "npx ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:revert -d src/database/data-source.ts",
    "db:migration:run": "npx ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:run -d src/database/data-source.ts",
    "db:seed:dev": "NODE_ENV=development npx ts-node ./src/database/seeds/index.ts",
    "db:seed:test": "NODE_ENV=test npx ts-node ./src/database/seeds/index.ts",
    "test:unit": "jest --config jest.unit.js",
    "test:integration": "jest --config jest.integration.js --detectOpenHandles --forceExit",
    "test:e2e": "jest  --config jest.e2e.js --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cross-var": "^1.1.0",
    "csrf-csrf": "^3.1.0",
    "graphql": "^16.10.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "leaked-handles": "^5.2.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "18.20.6",
    "yarn": "1.22.22"
  },
  "resolutions": {
    "cookie": "^0.7.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn run format",
      "yarn run lint"
    ]
  }
}
